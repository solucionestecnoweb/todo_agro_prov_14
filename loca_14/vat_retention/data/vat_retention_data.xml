<odoo>
    <data noupdate="0">
        
        <!-- Sequences for VAT retention purchase or suppliers-->
        <record id="seq_vat_retention_suppliers" model="ir.sequence">
            <field name="name">VAT Retention Voucher Number</field>
            <field name="code">purchase.vat.retention.voucher.number</field>
            <field name="implementation">standard</field>
            <field name="prefix">%(year)s%(month)s</field>
            <field name="padding">8</field>
            <field name="active" eval="1"/>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="company_id" eval="False"/>
        </record>

